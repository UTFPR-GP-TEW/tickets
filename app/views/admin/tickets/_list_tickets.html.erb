<table class="ui celled table">
  <thead>
  <tr class="center aligned">
    <th>#ID</th>
    <th>Titulo</th>
    <th>Descrição</th>
    <th colspan="3"> Ações</th>
  </tr>
  </thead>
  <tbody>
    <% @tickets.each do |ticket| %>
        <%= sync partial: 'ticket_row', resource: ticket %>
    <% end %>
    <%= sync_new partial: "ticket_row", resource: Ticket.new %>
  </tbody>
</table>